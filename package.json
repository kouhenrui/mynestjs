{
  "name": "mynest",
  "version": "0.0.1",
  "description": "nestjs web框架模板，包含了jwt签发，验证，mysql迁移表，rbac权限管理，redis缓存，常规操作日志记录，密钥轮询加密信息，统一返回格式，图形验证码，自定义错误抛出捕捉",
  "author": "khr",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@depyronick/nestjs-clickhouse": "^2.0.2",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/swagger": "^7.0.12",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.0.4",
    "@nestjsplus/knex": "^1.0.0",
    "@pardjs/nest-casbin": "^0.0.5",
    "@switchit/nestjs-casbin": "^0.1.5",
    "@types/webidl-conversions": "^7.0.0",
    "bcrypt": "^5.1.0",
    "casbin": "^5.26.1",
    "casbin-sequelize-adapter": "^2.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.11.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "log4js": "^6.9.1",
    "moment": "^2.29.4",
    "mongoose": "^7.3.2",
    "mysql2": "^3.4.3",
    "nestjs-casbin": "^2.0.8",
    "nestjs-knex": "^2.0.0",
    "nestjs-redis": "^1.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "stacktrace-js": "^2.0.2",
    "string-random": "^0.1.3",
    "svg-captcha": "^1.4.0",
    "typeorm": "^0.3.17",
    "typeorm-adapter": "^1.5.3",
    "webidl-conversions": "^7.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
